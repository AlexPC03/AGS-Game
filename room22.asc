function Liana_Look()
{
  player.Say("Una liana que cuelga de un árbol, creo");
}

function Liana_Interact()
{
  player.Walk(Liana.X, Liana.Y, eBlock, eWalkableAreas);
  player.AddInventory(LianaItem);
  Liana.Visible = false;
  player.Say("Esto puede servir de algo");
}

